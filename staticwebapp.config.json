{
	"responseOverrides": {
		"401": {
			"redirect": "/login",
			"statusCode": 302
		}
	},
	"globalHeaders": {
		"content-security-policy": "default-src 'self' 'unsafe-inline' ws://localhost:3000 https://login.microsoftonline.com https://fonts.googleapis.com https://fonts.gstatic.com https://*.core.windows.net ; img-src 'self' data: https://*.blob.core.windows.net"
	},
	"mimeTypes": {
		".json": "text/json"
	},
	"routes": [
		{
			"route": "/.auth/login/github",
			"statusCode": 404
		},
		{
			"route": "/.auth/login/twitter",
			"statusCode": 404
		},
		{
			"route": "/login",
			"rewrite": "/.auth/login/aad"
		},
		{
			"route": "/logout",
			"redirect": "/.auth/logout"
		},
		{
			"route": "/api/FileSystems",
			"methods": [
				"GET"
			],
			"allowedRoles": [
				"authenticated"
			]
		},
		{
			"route": "/api/TopLevelFolders",
			"methods": [
				"GET"
			],
			"allowedRoles": [
				"authenticated"
			]
		},
		{
			"route": "/api/TopLevelFolders",
			"methods": [
				"POST"
			],
			"allowedRoles": [
				"administrator"
			]
		},
		{
			"route": "/api/Configuration/CalculateFolderSizes",
			"methods": [
				"POST"
			],
			"allowedRoles": [
				"administrator"
			]
		}
	]
}